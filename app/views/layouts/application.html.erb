<!DOCTYPE html>
<html>
  <head>
    <title>Bookers2</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
    <header>
    <% if user_signed_in? %>
      <li>
        <%= link_to "Bookers", books_path %>
      </li>
      
      <li>
        <%= link_to "Home", destroy_user_session_path, method: :delete %>
      </li>
      <li>
        <%= link_to "User", destroy_user_session_path, method: :delete %>
      </li>
      <li>
        <%= link_to "Books", destroy_user_session_path, method: :delete %>
      </li>
      <li>
        <%= link_to "Logout", destroy_user_session_path, method: :delete %>
      </li>
      
    <% else %>
      <li>
        <%= link_to "Bookers", new_user_registration_path %>
      </li>
      
      <li>
        <%= link_to "Home", new_user_registration_path %>
      </li>
      <li>
        <%= link_to "About", new_user_session_path %>
      </li>
      <li>
        <%= link_to "Sign up", new_user_session_path %>
      </li>
      <li>
        <%= link_to "Log in", new_user_session_path %>
      </li>
      
      
    <% end %>
    </header>
    
    
    <!--footer--CopyRight Infratop.inc-->
      
   
    
    <%= yield %>
  </body>
</html>
